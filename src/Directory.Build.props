<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <Version>4.1.1</Version>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <LangVersion>latest</LangVersion>
    <Description>Low level .net wrapper for the SQL Server transport.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Copyright $([System.DateTime]::UtcNow.ToString(yyyy)). All rights reserved</Copyright>
    <PackageTags>NServiceBus, Native, Raw</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/NServiceBusExtensions/NServiceBus.Native/master/src/icon.png</PackageIconUrl>
    <PackageOutputPath>..\..\nugets</PackageOutputPath>
    <RepositoryUrl>https://github.com/NServiceBusExtensions/NServiceBus.Native</RepositoryUrl>
    <SignAssembly>true</SignAssembly>
    <NoWarn>1573</NoWarn>
    <AssemblyOriginatorKeyFile>../key.snk</AssemblyOriginatorKeyFile>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <DebugType>embedded</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <NoWarn>CS1591;CS1573</NoWarn>
  </PropertyGroup>
</Project>